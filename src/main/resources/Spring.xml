<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->
    <context:component-scan base-package="org.genspark"/>
    <context:annotation-config></context:annotation-config>
    <context:property-placeholder location="classpath:property.properties"/>

    <bean id="phone" class="org.genspark.Phone">
        <property name="mob" value="626 228 1111"/>
    </bean>
    <bean id="phone1" class="org.genspark.Phone">
        <property name="mob" value="626 228 1111"/>
    </bean>
    <bean id="phone2" class="org.genspark.Phone">
        <property name="mob" value="626 228 1111"/>
    </bean>
    <bean id="phone3" class="org.genspark.Phone">
        <property name="mob" value="626 228 1111"/>
    </bean>

    <bean id="student" class="org.genspark.Student">
        <property name="ph">
            <list>
                <ref bean="phone"/>
                <ref bean="phone2"/>
                <ref bean="phone3"/>
            </list>
        </property>
    </bean>

</beans>